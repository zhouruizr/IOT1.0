<script type="text/javascript">

    //判断页面是否有回车操作
    window.document.onkeydown = function (e) {
        e = e || window.event;
        if (e.keyCode == 13 || e.keyCode == 108) {
            document.getElementById('frmQuery').submit();
        }
    }
    //点击列表的编辑按钮，编辑操作
    function modify(id) {
        cleanData("buttoninfo");
        layer.open({
            type: 1,
            title: "<h3 class='ibox-title'>编辑按钮</h3>",
            area: ['680px', '400px'],
            content: $('#buttoninfo'),
            btn: ['保存', '取消'],
            yes: function () {
                save(id);
            },
            cancel: function (index) {
                layer.close(index);
            }
        });

        if (id) {//提供存着ID就是修改，则加载数据，不然打开空页面
            var loadindex = layer.load();
            loaddetail(id, loadindex)
        }
    }
    //跟进
    function follow(id) {
        cleanData("follow");//清空数据
        layer.open({
            type: 1,
            title: "<h3 class='ibox-title'>用户跟进</h3>",
            area: ['680px', '400px'],
            content: $('#follow'),
            btn: ['保存', '取消'],
            yes: function () {

            },
            cancel: function (index) {
                layer.close(index);
            }
        });
        //配置下拉控件chosen
        $(".chosen-select").chosen({
            no_results_text: "未找到此选项!",
            search_contains: true
        });
    }
    //试听
    function shiting(id) {
        cleanData("shiting");//清空数据
        layer.open({
            type: 1,
            title: "<h3 class='ibox-title'>安排试听</h3>",
            area: ['1000px', '600px'],
            content: $('#shiting'),
            btn: ['保存', '取消'],
            yes: function () {

            },
            cancel: function (index) {
                layer.close(index);
            }
        });

    }
    //报名
    function baoming(id) {
       // cleanData("baoming");//清空数据
        layer.open({
            type: 1,
            title: "<h3 class='ibox-title'>报名</h3>",
            area: ['1000px', '600px'],
            content: $('#baoming'),
            btn: ['结算', '取消'],
            yes: function () {

            },
            cancel: function (index) {
                layer.close(index);
            }
        });
    }

    //加入
    function jiaru(id) {
        // cleanData("baoming");//清空数据
        layer.open({
            type: 1,
            title: "<h3 class='ibox-title'>成为正式学员</h3>",
            area: ['800px', '600px'],
            content: $('#jiaru'),
            btn: ['成为正式学员', '取消'],
            yes: function () {

            },
            cancel: function (index) {
                layer.close(index);
            }
        });
    }
 //票据
    function piaoju(id) {
        //iframe窗
        parent.layer.open({
            type: 2,
            title: "票据打印",
            closeBtn: false,
            shade: [0],
            area: ['800px', '600px'],
            //maxmin: true, //开启最大化最小化按钮
            content: ['@Url.Action("InvoicePrint", "Enroll")'], //iframe的url，no代表不显示滚动条
            cancel: function () {
                layer.closeAll();
            }
        });


    }

</script>
